   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Create a 28x28 array filled with 0 (black)\n",
    "img = np.zeros((30, 30))\n",
    "i=1\n",
    "z=30\n",
    "while i!=29:\n",
    "    img[z-i,i]=255\n",
    "    img[15,i]=255\n",
    "    img[16,i]=255\n",
    "    i=i+1\n",
    "\n",
    "\n",
    "plt.imshow(img, cmap=\"gray\")\n",
    "plt.axis(\"off\")\n",
    "plt.show()\n"
   ]
  },
